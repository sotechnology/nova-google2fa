{
    "name": "sotechnology/nova-google2fa",
    "description": "Fork of carloscgo/nova-google2fa to make compatible with PHP 7.4",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0",
        "bacon/bacon-qr-code": "^2.0",
        "pragmarx/recovery": "^0.1.0",
        "pragmarx/google2fa-laravel": "1.*"
    },
    "autoload": {
        "psr-4": {
            "CarlosCGO\\Google2fa\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "CarlosCGO\\Google2fa\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
